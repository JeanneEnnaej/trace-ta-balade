<%= simple_form_for(walk) do |f| %>
   <%= f.input :title %>
  <%= f.input :num_km %>
  <%= f.input :duration %>
  <%= f.input :ratting, collection: [1, 2 , 3, 4, 5] %>
  <%= f.input :address%>
  <%= f.input :date %>
  <%= f.input :content %>
  <%= f.input :photos, as: :file %>

  <%= f.button :submit %>
<% end %>
