<%= simple_form_for(@walk) do |f| %>
   <%= f.input :title %>
  <%= f.input :num_km ,input_html: { min: '0', max: '9999', step: '0.5' }%>
  <%= f.input :duration, input_html: { min: '0', max: '9999', step: '0.5' }  %>
  <%= f.input :ratting, collection: [1, 2 , 3, 4, 5] %>
  <%= f.input :address%>
  <%= f.input :date %>
  <%= f.input :content %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.association :advantages, as: :radio_buttons%>
  <%= f.association :disadvantages, as: :radio_buttons %>
  <%= f.button :submit %>
<% end %>
